    <div id="SD2" >
        <svg class="chart"></svg>
        <script src="d3.min.js" charset="utf-8"></script>
        <script>

		var margin = {top: 20, right: 20, bottom: 20, left: 20},
			width = 600 - margin.left - margin.right,
			height = 300 - margin.top - margin.bottom;

		var x = d3.scale.ordinal()
			.rangeRoundBands([width,0],0.8);

		var y = d3.scale.linear()
			.range([height,0]);

		var xAxis = d3.svg.axis()
			.scale(x)
			.orient("bottom");

		var xAxis1 = d3.svg.axis()
			.scale(x)
			.orient("bottom");

		var yAxis = d3.svg.axis()
			.scale(y)
			.orient("left");

		var chart = d3.select(".chart")
			.attr("width", width + margin.left + margin.right)
			.attr("height", height + margin.top + margin.bottom)
			.append("g")
			.attr("transform", "translate(" + margin.left + "," + margin.top + ")");

		//d3.csv("http://localhost:8080/test.csv", type, function(error, data) {
		  var data = [
		{name: -0.1,  value: 3},
		{name: -0.09,  value: 0},
		{name: -0.08,  value: 2},
		{name: -0.07,  value: 3},
		{name: -0.06,  value: 0},
		{name: -0.05,  value: 5},
		{name: -0.04,  value: 13},
		{name: -0.03,  value: 28},
		{name: -0.02,  value: 49},
		{name: -0.01,  value: 93},
		{name: 0,  value: 201},
		{name: 0.01,  value: 181},
		{name: 0.02,  value: 114},
		{name: 0.03,  value: 58},
		{name: 0.04,  value: 25},
		{name: 0.05,  value: 10},
		{name: 0.06,  value: 4},
		{name: 0.07,  value: 3},
		{name: 0.08,  value: 3},
		{name: 0.09,  value: 0}
		  ];

		  var data1 = [
		{name: -0.1,  value: 3},
		{name: -0.09,  value: 1},
		{name: -0.08,  value: 0},
		{name: -0.07,  value: 0},
		{name: -0.06,  value: 3},
		{name: -0.05,  value: 6},
		{name: -0.04,  value: 7},
		{name: -0.03,  value: 9},
		{name: -0.02,  value: 41},
		{name: -0.01,  value: 106},
		{name: 0,  value: 231},
		{name: 0.01,  value: 219},
		{name: 0.02,  value: 93},
		{name: 0.03,  value: 42},
		{name: 0.04,  value: 17},
		{name: 0.05,  value: 8},
		{name: 0.06,  value: 6},
		{name: 0.07,  value: 0},
		{name: 0.08,  value: 2},
		{name: 0.09,  value: 1}
		];

		  x.domain(data.map(function(d) { return d.name; }));
		  y.domain([0, d3.max(data, function(d) { return d.value; })]);


		chart.append("g")
			  .attr("class", "x axis")
			  .attr("transform", "translate(0,"+height+")")
			  .call(xAxis);

		chart.append("g")
			  .attr("class", "x axis")
			  .attr("transform", "translate("+x.rangeBand()+","+height+")")
			  .call(xAxis1);

		   chart.selectAll(".bar")
			  .data(data)
			.enter().append("rect")
			  .attr("class", "bar")
			  .attr("x", function(d) { return x(d.name); })
			  .attr("y", function(d) { return y(d.value); })
			  .attr("fill", "mediumseagreen")
			  .attr("height", function(d) { return height - y(d.value); })
			  .attr("width", x.rangeBand())
			  .on('click', function(d,i){  alert( d.value ) ; })
			  .on('dblclick', function(d,i){  alert( d.name ) ; });

		   chart.selectAll(".bar1")
			  .data(data1)
			.enter().append("rect")
			  .attr("class", "bar1")
			  .attr("x", function(d) { return x(d.name)+x.rangeBand(); })
			  .attr("y", function(d) { return y(d.value); })
			  .attr("fill", "mediumturquoise")
			  .attr("height", function(d) { return height - y(d.value); })
			  .attr("width", x.rangeBand())
			  .on('click', function(d,i){  alert( d.value ); })
			  .on('dblclick', function(d,i){  alert( d.name ); });

		//});

		function type(d) {
		  d.value = +d.value; // coerce to number
		  return d;
		}

	</script>
    </div>

